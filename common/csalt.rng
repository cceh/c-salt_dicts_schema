         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">documents a single value in a predefined list of values. [22.5.4. Attribute List Specification]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="tei_model.descLike"/>
                  <ref name="tei_model.glossLike"/>
               </choice>
            </zeroOrMore>
            <optional>
               <ref name="tei_paramList"/>
            </optional>    
         </group>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.combinable.attributes"/>

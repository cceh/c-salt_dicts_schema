         <ref name="tei_att.pointing.attributes"/>
         <ref name="tei_att.typed.attribute.subtype"/>
         <ref name="tei_att.written.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Cologne Sanskrit Lexicon: The attribute type is used as a container to preserve the element's name (and further information) used in the source file.</a:documentation>
               <choice>
                  <value>addedInfo</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Monier source file: Used for additional information added by the editors of the digital edition</a:documentation>

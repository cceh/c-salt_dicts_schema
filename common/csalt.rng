                  <oneOrMore>
                     <choice>
                        <data type="anyURI">
                           <param name="pattern">[^/]+:.*</param>
                        </data>
                        <data type="Name">
                           <param name="pattern">.+:.+</param>
                        </data>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
